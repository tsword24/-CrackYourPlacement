Print all The Dupplicate characters in the string

#include<bits/stdc++.h>
using namespace std;
void dupstr(string s){
    unordered_map<char,int> count;
    for(int i=0;i<s.length();i++){
        count[s[i]]++;
    }
    for(auto it:count){
        if(it.second>1){
            cout<<it.first<<"count : "<<it.second;
            cout<<"\n";
        }
    }
}

int main(){
    string str;
    cin>>str;
    dupstr(str);
}
time:21 mins
